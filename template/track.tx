<script src="/js/base.js"></script>
<link rel="stylesheet" href="/css/base.css" />
: include include::google_analytics
</head>
<body>
<script>
var loading = false;
jQuery(function($) {
    $("#prev").click(function(){
        prev();
    });
    $("#next").click(function(){
        next();
    });
    $("#show").click(function() {
        show();
    });
    $("#content").tabs();
    get_list("<: $country :>", "<: $rank :>");
});
</script>
<header>
<h1><a href="/">CountDown LastFM Ranking (<: $country :>)</a></h1>
</header>
<article>
<div id="content">
    <ul>
    <li><a href="#play">Video</a></li>
    <li><a href="#list">Chart</a></li>
    <li><a href="#other">Other</a></li>
    </ul>
    <div id="play">
        <span id="info"></span>
        <div id="next_song"></div>
    </div>
    <div id="list"></div>
    <div id="other">
        <p>Do You Want to Play Which Countries Ranking ??</p>
        <ul class=".ui-icon-play">
        : $country == "jp" ? '<li>Japan</li>' | raw : '<li><a href="/track/jp">Japan</a></li>' | raw
        : $country == "us" ? "<li>United States</li>" | raw : '<li><a href="/track/us">United States</a></li>' | raw
        : $country == "uk" ? "<li>United Kingdom</li>" | raw : '<li><a href="/track/uk">United Kingdom</a></li>' | raw
        </ul>
    </div>
    <div id="current"></div>
    <div id="video"></div>
    <div id="operation" >
        <input type="button"  id="prev" value="Prev" />
        <input type="button"  id="next" value="Next" />
    </div>
</div>
</article>
<div id="fb-root"></div>
<script>(function(d, s, id) {
